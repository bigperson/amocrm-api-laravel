{
    "name": "bigperson/amocrm-api-laravel",
    "description": "An AmoCRM api bridge for Laravel 5",
    "type": "library",
    "require": {
        "linkprofit-cpa/amocrm-api": "^0.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Kartsev",
            "email": "anton@alarm.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bigperson\\AmoCrmApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bigperson\\AmoCrmApi\\AmoCrmApiServiceProvider"
            ]
        }
    }
}
